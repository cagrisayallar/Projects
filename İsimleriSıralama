using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Collections;

namespace DersDeneme
{
    class Program
    {
        static void Main(string[] args)
        {
            ArrayList A1 = new ArrayList();

            for (int i = 0; i < 10; i++)
            {

                Console.Write("Lutfen {0}'inci isimi giriniz : ", i + 1);
                string deger = Console.ReadLine();
                A1.Add(deger);
                
            }

            A1.Sort();
            A1.Reverse();

            for(int i=0;i<10;i++)
            {
                Console.WriteLine("{0} ", A1[i]);
            }
            Console.ReadLine();
        }

    }
}
